<%= simple_form_for [:dashboard, @collection] do |f| %>
  <%= f.input :title %>
  <%= render "dashboard/shared/form_field_visibility", f:f  %>
  <%= render "dashboard/shared/form_field_description", f:f  %>
  <%= render "dashboard/shared/form_field_category", f:f  %>
  <%= f.submit class: "btn-primary" %>
<% end %>
