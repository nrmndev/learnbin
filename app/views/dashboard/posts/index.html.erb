
<div class="bg-indigo-600 p-5 mb-3">
  <div class="container mx-auto">
      <%= render "shared/breadcrumbs", links: [
      { text: "Dashboard", url: dashboard_path },
      { text: "All Posts" }
      ]
    %>

    <%= render "dashboard/shared/title", title: "Posts" %>
  </div>
</div>

<div class="container mx-auto">
  <%= link_to new_dashboard_post_path, class:"inline-flex items-center btn-primary"  do %>
    <%= phosphor_icon "plus", class:"h-4 w-4 text-white"%> Create Post
  <% end  %>
  <%= render "dashboard/posts/index/table"  %>
</div>
